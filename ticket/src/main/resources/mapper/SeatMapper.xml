<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ticket.mapper.SeatMapper">
    <select id="findSeatByHallId" resultType="com.example.ticket.entity.Seat" parameterType="long">
        select * from t_seat where hall_id = #{hallId}
    </select>
</mapper>